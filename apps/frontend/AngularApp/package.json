{
  "name": "angular-app-v2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "startPowershell": "npx ng serve --ssl=true --ssl-key=$env:WML_CERT_KEY0 --ssl-cert=$env:WML_CERT0",
    "build:prod": " ng build -c production",
    "build:preview": " ng build -c preview",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "test-ci": "ng test --karma-conf=karma.conf.circle-ci.js",
    "lint": "ng lint --fix",
    "analyze:dev": "ng build -c development --stats-json && webpack-bundle-analyzer --port 9000 dist/angular-app-dev/stats.json",
    "analyze:preview": "ng build -c preview --stats-json && webpack-bundle-analyzer --port 9000 dist/angular-app-preview/stats.json",
    "analyze:prod": "ng build -c production --stats-json && webpack-bundle-analyzer --port 9000 dist/angular-app-prod/stats.json",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.6",
    "@angular/cdk": "~16.1.5",
    "@angular/common": "^16.1.6",
    "@angular/compiler": "^16.1.6",
    "@angular/core": "^16.1.6",
    "@angular/forms": "^16.1.6",
    "@angular/material": "~16.1.5",
    "@angular/platform-browser": "^16.1.6",
    "@angular/platform-browser-dynamic": "^16.1.6",
    "@angular/router": "^16.1.6",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@rxweb/reactive-form-validators": "^13.0.1",
    "@scarf/scarf": "^1.1.1",
    "@sentry/angular-ivy": "^7.56.0",
    "@sentry/tracing": "^7.56.0",
    "@windmillcode/angular-wml-button-zero": "^4.0.5",
    "@windmillcode/angular-wml-chips": "3.0.1",
    "@windmillcode/angular-wml-components-base": "^2.1.2",
    "@windmillcode/angular-wml-field": "2.0.1",
    "@windmillcode/angular-wml-file-manager": "2.0.1",
    "@windmillcode/angular-wml-form": "1.0.2",
    "@windmillcode/angular-wml-infinite-dropdown": "^1.1.0",
